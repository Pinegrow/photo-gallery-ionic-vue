{
  "name": "photo-gallery-ionic-vue",
  "version": "0.0.0",
  "type": "module",
  "homepage": "https://photo-gallery-ionic-vue.netlify.app",
  "repository": {
    "type": "git",
    "url": "https://github.com/pinegrow/photo-gallery-ionic-vue.git"
  },
  "description": "Build a photo gallery app that runs on iOS, Android, and the web - with just one codebase.",
  "scripts": {
    "dev": "vite",
    "now": "npm run build && npm run preview",
    "lighthouse": "npx unlighthouse --no-cache --site https://photo-gallery-ionic-vue.netlify.app",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit",
    "vue-devtools": "vue-devtools",
    "lint": "npm run lint:fix && npm run format",
    "lint:fix": "eslint --fix --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts ./ --ignore-path .gitignore",
    "format": "prettier --write \"**/*.{vue,js,jsx,cjs,mjs,ts,tsx,cts,mts,html,css,md,mdx,json}\" !public --ignore-path .gitignore"
  },
  "dependencies": {
    "@capacitor/android": "^6.0.0",
    "@capacitor/app": "6.0.0",
    "@capacitor/camera": "^6.0.0",
    "@capacitor/core": "6.0.0",
    "@capacitor/filesystem": "^6.0.0",
    "@capacitor/haptics": "6.0.0",
    "@capacitor/ios": "^6.0.0",
    "@capacitor/keyboard": "6.0.0",
    "@capacitor/preferences": "^6.0.0",
    "@capacitor/status-bar": "6.0.0",
    "@ionic/core": "^8.2.1",
    "@ionic/pwa-elements": "^3.3.0",
    "@ionic/vue": "^8.2.1",
    "@ionic/vue-router": "^8.2.1",
    "ionicons": "^7.4.0",
    "vue": "^3.4.27",
    "vue-router": "^4.3.2"
  },
  "devDependencies": {
    "@capacitor/cli": "^6.0.0",
    "@iconify/json": "^2.2.64",
    "@pinegrow/vite-plugin": "^3.0.68",
    "@rushstack/eslint-patch": "^1.2.0",
    "@types/node": "^20.14.2",
    "@unocss/preset-icons": "^0.60.4",
    "@vitejs/plugin-legacy": "^5.4.1",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vue/devtools": "^6.5.0",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "eslint": "^8.39.0",
    "eslint-plugin-vue": "^9.11.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "rollup-plugin-visualizer": "^5.12.0",
    "terser": "^5.31.0",
    "typescript": "5.1",
    "unocss": "^0.60.4",
    "unplugin-auto-import": "^0.17.6",
    "unplugin-vue-components": "^0.27.0",
    "vite": "^5.2.12",
    "vite-plugin-vue-devtools": "^7.3.6",
    "vitest": "^1.6.0",
    "vue-tsc": "^2.0.19"
  },
  "web-types": "web-types.json"
}
