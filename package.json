{
  "name": "photo-gallery-ionic-vue",
  "version": "0.0.0",
  "type": "module",
  "homepage": "https://photo-gallery-ionic-vue.netlify.app",
  "repository": {
    "type": "git",
    "url": "https://github.com/pinegrow/photo-gallery-ionic-vue.git"
  },
  "description": "Build a photo gallery app that runs on iOS, Android, and the web - with just one codebase.",
  "scripts": {
    "dev": "vite",
    "now": "npm run build && npm run preview",
    "lighthouse": "npx unlighthouse --no-cache --site https://photo-gallery-ionic-vue.netlify.app",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit",
    "vue-devtools": "vue-devtools",
    "lint": "npm run lint:fix && npm run format",
    "lint:fix": "eslint --fix ./",
    "format": "prettier --write \"**/*.{html,vue,svelte,astro,js,jsx,cjs,mjs,ts,tsx,cts,mts,css,md,mdx,json}\" !public --ignore-path .gitignore"
  },
  "dependencies": {
    "@capacitor/android": "^6.2.0",
    "@capacitor/app": "6.0.2",
    "@capacitor/camera": "^6.1.2",
    "@capacitor/core": "6.2.0",
    "@capacitor/filesystem": "^6.0.2",
    "@capacitor/haptics": "6.0.2",
    "@capacitor/ios": "^6.2.0",
    "@capacitor/keyboard": "6.0.3",
    "@capacitor/preferences": "^6.0.3",
    "@capacitor/status-bar": "6.0.2",
    "@ionic/core": "^8.4.1",
    "@ionic/pwa-elements": "^3.3.0",
    "@ionic/vue": "^8.4.1",
    "@ionic/vue-router": "^8.4.1",
    "@vueuse/core": "^12.4.0",
    "ionicons": "^7.4.0",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@capacitor/cli": "^6.2.0",
    "@iconify/json": "^2.2.295",
    "@nuxt/eslint-config": "^0.7.5",
    "@pinegrow/vite-plugin": "^3.0.69",
    "@types/node": "^22.10.6",
    "@unocss/preset-icons": "^65.4.0",
    "@vitejs/plugin-legacy": "^6.0.0",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/devtools": "^7.7.0",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^9.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.4.2",
    "rollup-plugin-visualizer": "^5.14.0",
    "terser": "^5.37.0",
    "typescript": "5.7",
    "unocss": "^65.4.0",
    "unplugin-auto-import": "^19.0.0",
    "unplugin-vue-components": "^28.0.0",
    "vite": "^6.0.7",
    "vite-plugin-vue-devtools": "^7.7.0",
    "vitest": "^2.1.8",
    "vue-tsc": "^2.2.0"
  },
  "web-types": "web-types.json"
}
